  "editor.tokenColorCustomizations": {
    "comments": "#233874",
    "keywords": "#000000",
    "functions": "#8000FF",
    "numbers": "#800000",
    "strings": "#0000FF",
    "types": "#008080",
    "variables": "#800000",
    "textMateRules": [
      {
        "name": "Keywords",
        "scope": "keyword.control",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "All keywords",
        "scope": "keyword.other",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "C++ keywords",
        "scope": "keyword.cpp",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Cast operators",
        "scope": "keyword.operator.cast",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Word-like operators, such as 'new' and 'delete'",
        "scope": "keyword.operator.wordlike.cpp",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "I added this to cover 'bool' in 'operator bool'",
        "scope": "entity.name.operator.type.cpp",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "this",
        "scope": "variable.language.this.cpp",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Function-like keywords",
        "scope": "keyword.operator.functionlike",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      },
      {
        "name": "All keywords",
        "scope": "keyword",
        "settings": {
          "foreground": "#000000",
        }
      },
      {
        "scope": "constant",
        "settings": {
          "foreground": "#000000",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "attribute": {
        "foreground": "#FF8000",
        "bold": true
      },
      "class": {"foreground": "#008080", "bold": true},
      "comment": {
        "foreground": "#233874",
        "italic": false,
      },
      "concept": {
        "foreground": "#B030B0",
        "bold": true
      },
      "enum": {"foreground": "#000080", "bold": true},
      "enumMember": {"foreground": "#0000C0", "bold": true},
      "function.classScope.static": {
        "foreground": "#008000",
        "underline": true
      },
      "function.declaration": {
        "foreground": "#4E0077",
        "bold": true,
      },
      "interface": {
        "foreground": "#008080", 
        "bold": true
      },
      "macro": "#008000",
      "macro.declaration": {"foreground": "#008000", "bold": true},
      "method": "#008000",
      "method.virtual": {
        "foreground": "#008000",
        "italic": true
      },
      "method.declaration": {
        "foreground": "#008000",
        "bold": true
      },
      "namespace": {"foreground": "#800000", "bold": true},
      "parameter": {"foreground": "#804000", "italic": true},
      "property": "#008000",
      "struct": {"foreground": "#008080", "bold": true},
      "type": {"foreground": "#800000", "bold": true},
      "type.dependentName": {
        "foreground": "#008080",
        "bold": true,
        "italic": true
      },
      "typeParameter": {"foreground": "#FF8000", "bold": true},
      "unknown": {
        "foreground": "#8000FF",
        "italic": true
      },
      "variable": "#800000",
      "variable.functionScope": "#800000",
      "variable.classScope.static": {
        "foreground": "#008000",
        "underline": true
      },
      "variable.fileScope": "#8080FF",
      "variable.globalScope": "#8080FF",
      "variable.usedAsMutableReference": {
        "foreground": "#c86496",
        "bold": true
      },
    }
  },
