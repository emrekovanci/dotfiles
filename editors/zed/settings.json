{
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
  "show_wrap_guides": false,
  "cursor_shape": "block",
  "current_line_highlight": "none",
  "theme_overrides": {
    "Solarized Dark": {
      "background": "#001E25",
      "panel.background": "#001E25",
      "editor.background": "#001E25",
      "editor.gutter.background": "#001E25",
      "status_bar.background": "#001E25",
      "ghost_element.active": "#001E25",
      "title_bar.background": "#001E25",
      "tab_bar.background": "#001E25",
      "tab.active_background": "#001E25",
      "tab.inactive_background": "#001E25",
      "hint.background": "#001E25",
      "elevated_surface.background": "#001E25",
    },
  },
  "preview_tabs": {
    "enable_preview_file_from_code_navigation": false,
    "enable_preview_from_multibuffer": false,
    "enable_preview_from_project_panel": false,
    "enabled": false,
  },
  "tab_bar": {
    "show_tab_bar_buttons": false,
    "show_nav_history_buttons": false,
  },
  "toolbar": {
    "agent_review": false,
    "selections_menu": false,
    "quick_actions": false,
    "breadcrumbs": false,
  },
  "buffer_font_family": "Lilex",
  "outline_panel": {
    "indent_guides": {
      "show": "never",
    },
  },
  "project_panel": {
    "entry_spacing": "standard",
    "hide_root": false,
    "auto_reveal_entries": false,
    "sticky_scroll": false,
    "indent_guides": {
      "show": "never",
    },
  },
  "indent_guides": {
    "enabled": false,
  },
  "disable_ai": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "base_keymap": "VSCode",
  "icon_theme": "Material Icon Theme",
  "ui_font_size": 18.0,
  "buffer_font_size": 17.0,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Solarized Dark",
  },
  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "pwsh",
        "args": ["-NoLogo"],
        "title_override": "pwsh",
      },
    },
  },
  "lsp": {
    "clangd": {
      "binary": {
        "path": "C:/Program Files/LLVM/bin/clangd.exe",
        "ignore_system_version": true,
      },
    },
  },
}
