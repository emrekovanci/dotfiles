{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command": "toggleFocusMode",
            "id": "User.toggleFocusMode"
        },
        {
            ////////////////////////////////////////////////////////////////////
            // I wasted 2-3 hours tring to fix that shit.......................
            ////////////////////////////////////////////////////////////////////
            // example issue: https://github.com/microsoft/terminal/issues/3483
            // CSI u Protocol Specification
            // \u001b[ + key id + ; + modifier + u
            // https://sw.kovidgoyal.net/kitty/keyboard-protocol/
            // escape codes like \x1b must be written as \u001b: https://learn.microsoft.com/en-us/windows/terminal/customize-settings/actions#send-input
            // linux "showkey -a"
            "command":
            {
                "action": "sendInput",
                "input": "\u001b[32;5u"
            },
            "id": "fix_neovim_ctrl_space"
        },
        {
            "command":
            {
                "action": "sendInput",
                "input": "\u001b[34;5u"
            },
            "id": "fix_neovim_ctrl_quote"
        }
    ],
    "alwaysShowNotificationIcon": false,
    "centerOnLaunch": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{8da5f3f5-632f-4b5d-a72a-2f8e6c5cc726}",
    "disableAnimations": false,
    "initialCols": 150,
    "initialRows": 40,
    "keybindings":
    [
        {
            "id": "fix_neovim_ctrl_space",
            "keys": "ctrl+space"
        },
        {
            "id": "fix_neovim_ctrl_quote",
            "keys": "ctrl+\""
        },
        {
            "id": "User.copy.644BA8F2",
            "keys": "ctrl+shift+c"
        },
        {
            "id": "User.paste",
            "keys": "ctrl+shift+v"
        },
        {
            "id": "User.splitPane.A6751878",
            "keys": "alt+shift+d"
        },
        {
            "id": "Terminal.ResizePaneDown",
            "keys": "ctrl+down"
        },
        {
            "id": "User.toggleFocusMode",
            "keys": "ctrl+f12"
        },
        {
            "id": "Terminal.ResizePaneUp",
            "keys": "ctrl+up"
        }
    ],
